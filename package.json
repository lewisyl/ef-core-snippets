{
    "name": "ef-core-snippets",
    "displayName": "EF Core Snippets",
    "description": "Entity Framework Core snippet generator for EF Core v 2.2.0",
    "version": "1.0.0",
    "icon": "icon.png",
    "publisher": "LewisYLiu",
    "repository": {
        "type": "git",
        "url": "https://github.com/lewisyl/ef-core-snippets"
    },
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "json",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "razor",
                "path": "./snippets/snippets.json"
